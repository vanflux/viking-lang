{
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "src/entry.ts"
  ],
  "compilerOptions": {
    "target": "es2022",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
